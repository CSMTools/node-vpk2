{
  "name": "vpk2",
  "version": "0.2.0",
  "description": "Parser and extractor for the Valve Pack Format",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CSMTools/node-vpk2.git"
  },
  "engines": {
    "node": ">=13.2.0"
  },
  "keywords": [
    "vpk",
    "Valve"
  ],
  "author": "Forward Command Post (http://fwdcp.net)",
  "contributors": [
    "Andrea Matt√® (andreamatte97@gmail.com)",
    "Albin Hedwall (albin.hedwall@gmail.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CSMTools/node-vpk2/issues"
  },
  "homepage": "https://github.com/CSMTools/node-vpk2#readme",
  "dependencies": {
    "crc": "^3.3.0",
    "jbinary": "^2.1.5"
  },
  "devDependencies": {
    "@types/crc": "^3.8.0",
    "@types/jbinary": "^2.1.1",
    "typescript": "^5.1.3"
  }
}
